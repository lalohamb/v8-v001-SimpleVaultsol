{
  "name": "sentinel-cronos-ai-vault-service",
  "version": "3.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .next",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:contract": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/contract-integration.test.ts",
    "verify-env": "tsx tests/verify-test-env.ts",
    "verify-e2e": "tsx tests/verify-e2e-setup.ts",
    "test:api": "tsx tests/api-test.ts",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/e2e-agents.test.ts --testTimeout=60000",
    "test:e2e:run": "tsx tests/run-e2e.ts",
    "test:all": "npm run lint && npm run test && npm run test:api",
    "test:full": "npm run test:all && npm run build",
    "test:complete": "npm run test:contract && npm run test:e2e:run"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "ethers": "^6.13.0",
    "express": "^4.19.0",
    "openai": "^4.67.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.3",
    "hardhat": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.8.0"
  }
}
